#! /usr/bin/vvp
:ivl_version "10.2 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x25b1300 .scope module, "sram_tb" "sram_tb" 2 6;
 .timescale -9 -12;
P_0x25a9760 .param/l "col" 0 2 26, +C4<00000000000000000000000000000000>;
P_0x25a97a0 .param/l "run_cycle" 0 2 25, +C4<00000000000000000000000001000000>;
v0x25cc830_0 .var "A", 10 0;
v0x25cc910_0 .var "CEN_EXT", 0 0;
v0x25cc9b0_0 .var "CEN_Q", 0 0;
v0x25ccab0_0 .var "CLK", 0 0;
v0x25ccb80 .array "D_2D", 0 63, 31 0;
v0x25ccc20_0 .var "D_tmp", 31 0;
v0x25cccc0_0 .net "Q", 31 0, L_0x25cd6d0;  1 drivers
v0x25ccd90_0 .var "WEN_EXT", 0 0;
v0x25cce30_0 .var "WEN_Q", 0 0;
v0x25ccf90_0 .var/i "b", 31 0;
v0x25cd030_0 .var/i "captured_data", 31 0;
v0x25cd110_0 .var/i "error", 31 0;
v0x25cd1f0_0 .var/i "t", 31 0;
v0x25cd2d0_0 .var/i "x_file", 31 0;
v0x25cd3b0_0 .var/i "x_scan_file", 31 0;
S_0x25b1480 .scope module, "sram_instance" "sram_32b_w2048" 2 28, 3 3 0, S_0x25b1300;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "CLK"
    .port_info 1 /INPUT 32 "D"
    .port_info 2 /OUTPUT 32 "Q"
    .port_info 3 /INPUT 1 "CEN"
    .port_info 4 /INPUT 1 "WEN"
    .port_info 5 /INPUT 11 "A"
P_0x25b1650 .param/l "num" 0 3 11, +C4<00000000000000000000100000000000>;
L_0x25cd6d0 .functor BUFZ 32, L_0x25cd490, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>;
v0x2596d40_0 .net "A", 10 0, v0x25cc830_0;  1 drivers
v0x25cbe00_0 .net "CEN", 0 0, v0x25cc9b0_0;  1 drivers
v0x25cbec0_0 .net "CLK", 0 0, v0x25ccab0_0;  1 drivers
v0x25cbf90_0 .net "D", 31 0, v0x25ccc20_0;  1 drivers
v0x25cc070_0 .net "Q", 31 0, L_0x25cd6d0;  alias, 1 drivers
v0x25cc1a0_0 .net "WEN", 0 0, v0x25cce30_0;  1 drivers
v0x25cc260_0 .net *"_s0", 31 0, L_0x25cd490;  1 drivers
v0x25cc340_0 .net *"_s2", 12 0, L_0x25cd5b0;  1 drivers
L_0x7f3245866018 .functor BUFT 1, C4<00>, C4<0>, C4<0>, C4<0>;
v0x25cc420_0 .net *"_s5", 1 0, L_0x7f3245866018;  1 drivers
v0x25cc590_0 .var "add_q", 10 0;
v0x25cc670 .array "memory", 0 2047, 31 0;
E_0x25a7ad0 .event posedge, v0x25cbec0_0;
L_0x25cd490 .array/port v0x25cc670, L_0x25cd5b0;
L_0x25cd5b0 .concat [ 11 2 0 0], v0x25cc590_0, L_0x7f3245866018;
    .scope S_0x25b1480;
T_0 ;
    %wait E_0x25a7ad0;
    %load/vec4 v0x25cbe00_0;
    %nor/r;
    %load/vec4 v0x25cc1a0_0;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %load/vec4 v0x2596d40_0;
    %assign/vec4 v0x25cc590_0, 0;
T_0.0 ;
    %load/vec4 v0x25cbe00_0;
    %nor/r;
    %load/vec4 v0x25cc1a0_0;
    %nor/r;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x25cbf90_0;
    %load/vec4 v0x2596d40_0;
    %pad/u 13;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x25cc670, 0, 4;
T_0.2 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x25b1300;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x25ccab0_0, 0, 1;
    %pushi/vec4 0, 0, 11;
    %store/vec4 v0x25cc830_0, 0, 11;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x25ccc20_0, 0, 32;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x25cc910_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x25ccd90_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x25b1300;
T_2 ;
    %vpi_call 2 39 "$dumpfile", "sram_tb.vcd" {0 0 0};
    %vpi_call 2 40 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x25b1300 {0 0 0};
    %vpi_func 2 42 "$fopen" 32, "activation.txt", "r" {0 0 0};
    %store/vec4 v0x25cd2d0_0, 0, 32;
    %vpi_func 2 45 "$fscanf" 32, v0x25cd2d0_0, "%s", v0x25cd030_0 {0 0 0};
    %store/vec4 v0x25cd3b0_0, 0, 32;
    %vpi_func 2 46 "$fscanf" 32, v0x25cd2d0_0, "%s", v0x25cd030_0 {0 0 0};
    %store/vec4 v0x25cd3b0_0, 0, 32;
    %vpi_func 2 47 "$fscanf" 32, v0x25cd2d0_0, "%s", v0x25cd030_0 {0 0 0};
    %store/vec4 v0x25cd3b0_0, 0, 32;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x25ccab0_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x25ccab0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x25ccd90_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x25ccab0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x25cd1f0_0, 0, 32;
T_2.0 ;
    %load/vec4 v0x25cd1f0_0;
    %cmpi/s 63, 0, 32;
    %jmp/0xz T_2.1, 5;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x25ccab0_0, 0, 1;
    %load/vec4 v0x25cc830_0;
    %addi 1, 0, 11;
    %store/vec4 v0x25cc830_0, 0, 11;
    %pushi/vec4 31, 0, 32;
    %store/vec4 v0x25ccf90_0, 0, 32;
T_2.2 ;
    %pushi/vec4 0, 0, 32;
    %load/vec4 v0x25ccf90_0;
    %cmp/s;
    %flag_or 5, 4;
    %jmp/0xz T_2.3, 5;
    %vpi_func 2 62 "$fscanf" 32, v0x25cd2d0_0, "%1b", &PV<v0x25ccc20_0, v0x25ccf90_0, 1> {0 0 0};
    %store/vec4 v0x25cd3b0_0, 0, 32;
    %load/vec4 v0x25ccf90_0;
    %subi 1, 0, 32;
    %store/vec4 v0x25ccf90_0, 0, 32;
    %jmp T_2.2;
T_2.3 ;
    %load/vec4 v0x25ccc20_0;
    %ix/getv/s 4, v0x25cd1f0_0;
    %store/vec4a v0x25ccb80, 4, 0;
    %vpi_call 2 65 "$display", "%2d-th written data is %h", v0x25cd1f0_0, v0x25ccc20_0 {0 0 0};
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x25ccab0_0, 0, 1;
    %load/vec4 v0x25cd1f0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x25cd1f0_0, 0, 32;
    %jmp T_2.0;
T_2.1 ;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x25ccab0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x25ccd90_0, 0, 1;
    %pushi/vec4 0, 0, 11;
    %store/vec4 v0x25cc830_0, 0, 11;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x25ccab0_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x25ccab0_0, 0, 1;
    %load/vec4 v0x25cc830_0;
    %addi 1, 0, 11;
    %store/vec4 v0x25cc830_0, 0, 11;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x25cd110_0, 0, 32;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x25ccab0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x25cd1f0_0, 0, 32;
T_2.4 ;
    %load/vec4 v0x25cd1f0_0;
    %cmpi/s 63, 0, 32;
    %jmp/0xz T_2.5, 5;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x25ccab0_0, 0, 1;
    %load/vec4 v0x25cc830_0;
    %addi 1, 0, 11;
    %store/vec4 v0x25cc830_0, 0, 11;
    %ix/getv/s 4, v0x25cd1f0_0;
    %load/vec4a v0x25ccb80, 4;
    %load/vec4 v0x25cccc0_0;
    %cmp/e;
    %jmp/0xz  T_2.6, 4;
    %vpi_call 2 82 "$display", "%2d-th read data is %h --- Data matched", v0x25cd1f0_0, v0x25cccc0_0 {0 0 0};
    %jmp T_2.7;
T_2.6 ;
    %vpi_call 2 84 "$display", "%2d-th read data is %h --- Data ERROR !!!", v0x25cd1f0_0, v0x25cccc0_0 {0 0 0};
    %load/vec4 v0x25cd110_0;
    %addi 1, 0, 32;
    %store/vec4 v0x25cd110_0, 0, 32;
T_2.7 ;
    %delay 20000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x25ccab0_0, 0, 1;
    %load/vec4 v0x25cd1f0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x25cd1f0_0, 0, 32;
    %jmp T_2.4;
T_2.5 ;
    %vpi_call 2 92 "$display", "###### Total %2d errors are detected ######", v0x25cd110_0 {0 0 0};
    %delay 10000, 0;
    %vpi_call 2 93 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x25b1300;
T_3 ;
    %wait E_0x25a7ad0;
    %load/vec4 v0x25ccd90_0;
    %assign/vec4 v0x25cce30_0, 0;
    %load/vec4 v0x25cc910_0;
    %assign/vec4 v0x25cc9b0_0, 0;
    %jmp T_3;
    .thread T_3;
# The file index is used to find the file name in the following table.
